package com.cnmts.baseinfo.exception;

/**
 * 车厢编号已经存在
 * 
 * @author 王璞
 * @date 2017年1月13日 下午3:06:01
 * @version 1.0
 */
public class CarNoAlreadExistException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2328849203297750972L;

	private String carNo;

	public CarNoAlreadExistException(String carNo) {
		super(carNo);
		this.carNo = carNo;
	}

	public String getCarNo() {
		return carNo;
	}

}
